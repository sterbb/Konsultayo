<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" style="-fx-background-color: #525252;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SanMigLight_MedicalService.PrintService">
   <children>
      <Label layoutX="41.0" layoutY="107.0" prefHeight="78.0" prefWidth="218.0" text="Thank you! We will contact you once your request has been approved." textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="SansSerif Bold" size="16.0" />
         </font>
      </Label>
      <ImageView fx:id="iconPrint" fitHeight="117.0" fitWidth="101.0" layoutX="100.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Images/logo.png" />
         </image>
      </ImageView>
      <Button fx:id="btnCancel" layoutX="14.0" layoutY="185.0" mnemonicParsing="false" onAction="#cancelPrint" prefHeight="50.0" prefWidth="123.0" style="-fx-background-color: gray; -fx-background-radius: 50;" stylesheets="@style.css" text="Back" textFill="WHITE">
         <font>
            <Font name="The Bold Font" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnPrint" layoutX="163.0" layoutY="185.0" mnemonicParsing="false" onAction="#printService" prefHeight="50.0" prefWidth="123.0" style="-fx-background-color: #3e9157; -fx-background-radius: 50;" stylesheets="@style.css" text="Print" textFill="WHITE">
         <font>
            <Font name="The Bold Font" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
