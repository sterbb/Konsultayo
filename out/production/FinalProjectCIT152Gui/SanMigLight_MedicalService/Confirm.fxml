<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: #525252;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SanMigLight_MedicalService.Confirm">
   <children>
      <Label layoutX="76.0" layoutY="81.0" prefHeight="50.0" prefWidth="177.0" text="Confirm Selection?" textFill="WHITE">
         <font>
            <Font name="SansSerif Bold" size="19.0" />
         </font>
      </Label>
      <ImageView fx:id="iconConfirm" fitHeight="100.0" fitWidth="87.0" layoutX="107.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Images/logo.png" />
         </image>
      </ImageView>
      <Button fx:id="btnCancel" layoutX="14.0" layoutY="125.0" mnemonicParsing="false" onAction="#Cancel" prefHeight="50.0" prefWidth="123.0" style="-fx-background-color: gray; -fx-background-radius: 50;" stylesheets="@style.css" text="Cancel" textFill="WHITE">
         <font>
            <Font name="The Bold Font" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnProceed3" layoutX="164.0" layoutY="125.0" mnemonicParsing="false" onAction="#Proceed" prefHeight="50.0" prefWidth="123.0" style="-fx-background-color: #3e9157; -fx-background-radius: 50;" stylesheets="@style.css" text="Proceed" textFill="WHITE">
         <font>
            <Font name="The Bold Font" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
